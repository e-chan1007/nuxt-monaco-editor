{
  "name": "nuxt-monaco-editor",
  "description": "Integrate monaco-editor with Nuxt (3)",
  "keywords": [
    "monaco-editor",
    "nuxt"
  ],
  "author": "e-chan1007",
  "homepage": "https://e-chan1007.github.io/nuxt-monaco-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/e-chan1007/nuxt-monaco-editor.git"
  },
  "version": "1.4.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "types": "./dist/types.d.mts"
    },
    "./dist/*": {
      "import": "./dist/*"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "postpack": "pinst --enable",
    "postinstall": "husky install && playwright install",
    "build": "nuxt-module-build build && yarn loc:build",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts,.vue . && remark . -q",
    "test": "nuxt test",
    "prepack": "pinst --disable && nuxt-module-build build && yarn loc:build",
    "release": "standard-version",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt prepare playground && yarn loc:build",
    "docs:dev": "nuxt dev docs",
    "docs:generate": "nuxt generate docs",
    "docs:prepare": "nuxt prepare docs",
    "docs:preview": "nuxt preview docs",
    "loc:build": "jiti scripts/build-loc.ts"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3",
    "defu": "^6.1.4",
    "vite-plugin-static-copy": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nuxt/content": "^3.6.3",
    "@nuxt/eslint-config": "^1.8.0",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/test-utils": "3.19.2",
    "@vue/test-utils": "^2.4.6",
    "better-sqlite3": "^12.2.0",
    "eslint": "^9.33.0",
    "execa": "^9.6.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "monaco-editor": "^0.52.2",
    "nuxt": "^4.0.3",
    "nuxt-content-twoslash": "^0.1.2",
    "pinst": "^3.0.0",
    "playwright": "^1.54.2",
    "playwright-core": "^1.54.2",
    "remark-cli": "^12.0.1",
    "remark-lint": "^10.0.1",
    "remark-preset-lint-recommended": "^7.0.1",
    "shadcn-docs-nuxt": "patch:shadcn-docs-nuxt@npm%3A1.1.2#~/.yarn/patches/shadcn-docs-nuxt-npm-1.1.2-b9d2f65cf2.patch",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "peerDependencies": {
    "monaco-editor": "*"
  },
  "resolutions": {
    "@unhead/vue": "^2"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "mixed"
      ]
    ]
  },
  "workspaces": [
    "playground"
  ],
  "packageManager": "yarn@4.5.1"
}
